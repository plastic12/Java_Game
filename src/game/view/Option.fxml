<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.view.OptionController">
   <children>
      <Pane prefHeight="560.0" prefWidth="602.0">
         <children>
            <ImageView fitHeight="560.0" fitWidth="602.0" pickOnBounds="true">
               <image>
                  <Image url="@background.png" />
               </image>
            </ImageView>
            <VBox layoutX="206.0" layoutY="88.0" spacing="30.0">
               <children>
                  <CheckBox fx:id="music" layoutX="206.0" layoutY="88.0" mnemonicParsing="false" text="Music">
                     <font>
                        <Font name="Agency FB" size="40.0" />
                     </font>
                  </CheckBox>
                  <CheckBox fx:id="dialog" layoutX="206.0" layoutY="169.0" mnemonicParsing="false" text="Dialog">
                     <font>
                        <Font name="Agency FB" size="40.0" />
                     </font>
                  </CheckBox>
                  <CheckBox fx:id="color" layoutX="206.0" layoutY="250.0" mnemonicParsing="false" text="Color">
                     <font>
                        <Font name="Agency FB" size="40.0" />
                     </font>
                  </CheckBox>
               </children>
            </VBox>
            <Label layoutX="255.0" layoutY="379.0" onMouseClicked="#backMenu" onMouseEntered="#mouseIn" onMouseExited="#mouseOut" text="Back">
               <font>
                  <Font name="Agency FB" size="40.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
