<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.view.GameOverController">
   <children>
      <Pane prefHeight="560.0" prefWidth="602.0">
         <children>
            <ImageView fitHeight="560.0" fitWidth="602.0" layoutX="0.0" layoutY="0.0" pickOnBounds="true">
               <image>
                  <Image url="@background.png" />
               </image>
            </ImageView>
            <Label alignment="CENTER" layoutX="132.0" layoutY="130.0" text="Game Over">
               <font>
                  <Font name="Agency FB" size="64.0" />
               </font></Label>
             <Label alignment="CENTER" layoutX="205.0" layoutY="229.0" prefHeight="29.0" text="Score:">
               <font>
                  <Font name="Agency FB" size="36.0" />
               </font>
            </Label>
            <Label fx:id="scoreLabel" layoutX="316.0" layoutY="229.0" prefHeight="29.0" prefWidth="91.0" text="1234">
               <font>
                  <Font name="Agency FB" size="36.0" />
               </font>
            </Label>
            <Label fx:id="restart" alignment="CENTER" layoutX="240.0" layoutY="336.0" onMouseClicked="#restart" onMouseEntered="#mouseIn" onMouseExited="#mouseOut" text="Restart">
               <font>
                  <Font name="Agency FB" size="36.0" />
               </font>
            </Label>
            <Label fx:id="home" alignment="CENTER" layoutX="185.0" layoutY="394.0" onMouseClicked="#menu" onMouseEntered="#mouseIn" onMouseExited="#mouseOut" text="Back to menu">
               <font>
                  <Font name="Agency FB" size="36.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
