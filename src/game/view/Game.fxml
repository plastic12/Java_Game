        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="48.0" prefWidth="600.0">
               <children>
                  <HBox layoutX="39.0" layoutY="12.0">
                     <children>
                        <Label fx:id="healthLabel" layoutX="39.0" layoutY="12.0" text="Health" />
